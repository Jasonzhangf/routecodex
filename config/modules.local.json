{
  "modules": {
    "httpserver": {
      "enabled": true,
      "config": {
        "moduleType": "http-server",
        "port": 5506,
        "host": "127.0.0.1",
        "cors": { "origin": "*", "credentials": true },
        "timeout": 30000,
        "bodyLimit": "10mb",
        "enableMetrics": true,
        "enableHealthChecks": true
      }
    },
    "openairouter": {
      "enabled": true,
      "config": {
        "moduleType": "openai-router",
        "basePath": "/v1/openai",
        "enableStreaming": true,
        "enablePipeline": false,
        "rateLimit": { "enabled": false, "requestsPerMinute": 60 }
      }
    }
  }
}

