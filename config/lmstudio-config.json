{
  "server": {
    "port": 5506,
    "host": "localhost"
  },
  "logging": {
    "level": "info",
    "format": "json"
  },
  "providers": {
    "lmstudio": {
      "type": "lmstudio",
      "baseUrl": "http://localhost:1234",
      "apiKey": "lm-studio-api-key-1234567890abcdef",
      "models": {
        "qwen3-4b-thinking-2507-mlx": {
          "maxTokens": 262144,
          "temperature": 0.7,
          "supportsStreaming": true,
          "supportsTools": true
        },
        "gpt-oss-20b-mlx": {
          "maxTokens": 262144,
          "temperature": 0.7,
          "supportsStreaming": true,
          "supportsTools": true
        },
        "qwen3-30b-a3b-instruct-2507-mlx": {
          "maxTokens": 262144,
          "temperature": 0.7,
          "supportsStreaming": true,
          "supportsTools": true
        },
        "qwen3-coder-480b-a35b-instruct-mlx": {
          "maxTokens": 262144,
          "temperature": 0.7,
          "supportsStreaming": true,
          "supportsTools": true
        },
        "seed-oss-36b-instruct": {
          "maxTokens": 262144,
          "temperature": 0.7,
          "supportsStreaming": true,
          "supportsTools": true
        }
      },
      "timeout": 60000,
      "retryAttempts": 3
    }
  },
  "routing": {
    "default": "lmstudio",
    "models": {
      "gpt-4": "qwen3-30b-a3b-instruct-2507-mlx",
      "gpt-4-turbo": "qwen3-30b-a3b-instruct-2507-mlx",
      "gpt-3.5-turbo": "qwen3-4b-thinking-2507-mlx",
      "claude-3-haiku": "qwen3-4b-thinking-2507-mlx",
      "claude-3-sonnet": "qwen3-30b-a3b-instruct-2507-mlx",
      "qwen3-4b-thinking-2507-mlx": "qwen3-4b-thinking-2507-mlx",
      "qwen3-30b-a3b-instruct-2507-mlx": "qwen3-30b-a3b-instruct-2507-mlx",
      "qwen3-coder-480b-a35b-instruct-mlx": "qwen3-coder-480b-a35b-instruct-mlx",
      "seed-oss-36b-instruct": "seed-oss-36b-instruct",
      "gpt-oss-20b-mlx": "gpt-oss-20b-mlx"
    }
  },
  "features": {
    "tools": {
      "enabled": true,
      "maxTools": 10
    },
    "streaming": {
      "enabled": true,
      "chunkSize": 1024
    },
    "oauth": {
      "enabled": true,
      "providers": ["qwen", "iflow"]
    }
  },
  "security": {
    "cors": {
      "enabled": true,
      "origins": ["http://localhost:3000", "http://localhost:8080"]
    },
    "rateLimit": {
      "enabled": true,
      "requests": 100,
      "windowMs": 60000
    }
  }
}