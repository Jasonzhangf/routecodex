{
  "compatibility": {
    "enabled": true,
    "mode": "dry-run",
    "simulateTransformation": true,
    "config": {
      "transformationRules": [
        {
          "id": "openai-to-lmstudio",
          "source": "openai",
          "target": "lmstudio",
          "mappings": {
            "model": "model",
            "messages": "messages",
            "temperature": "temperature",
            "max_tokens": "max_tokens"
          }
        }
      ]
    }
  },
  "provider": {
    "enabled": true,
    "mode": "dry-run",
    "simulateHttpRequest": true,
    "simulateResponse": true,
    "config": {
      "responseSimulation": {
        "latency": {
          "min": 100,
          "max": 2000,
          "distribution": "normal"
        },
        "errorRate": 0.02,
        "successRate": 0.98
      },
      "providers": {
        "lmstudio": {
          "baseUrl": "http://localhost:1234",
          "timeout": 30000,
          "retryAttempts": 3
        },
        "qwen": {
          "baseUrl": "https://chat.qwen.ai",
          "timeout": 45000,
          "retryAttempts": 2
        },
        "openai": {
          "baseUrl": "https://api.openai.com/v1",
          "timeout": 60000,
          "retryAttempts": 1
        }
      }
    }
  }
}
