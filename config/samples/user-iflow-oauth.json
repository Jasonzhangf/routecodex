{
  "version": "1.0.0",
  "httpserver": { "port": 5524, "host": "0.0.0.0" },
  "virtualrouter": {
    "outputProtocol": "openai",
    "providers": {
      "iflow": {
        "id": "iflow",
        "enabled": true,
        "type": "iflow",
        "baseURL": "https://apis.iflow.cn/v1",
        "apiKey": [],
        "oauth": {
          "default": {
            "clientId": "${IFLOW_CLIENT_ID}",
            "deviceCodeUrl": "https://iflow.cn/oauth/device/code",
            "tokenUrl": "https://iflow.cn/oauth/token",
            "scopes": ["inference"],
            "tokenFile": "~/.routecodex/tokens/iflow-default.json"
          }
        },
        "models": {
          "qwen3-coder-plus": {},
          "deepseek-v3.2": {},
          "glm-4.6": {},
          "kimi-k2-0905": {}
        }
      }
    },
    "routing": {
      "default": ["iflow.iflow-general"],
      "coding": [],
      "longcontext": [],
      "thinking": [],
      "tools": [],
      "vision": [],
      "websearch": [],
      "background": []
    }
  }
}
