{
  "version": "1.0.0",
  "httpserver": { "port": 5556, "host": "0.0.0.0" },
  "virtualrouter": {
    "inputProtocol": "openai",
    "outputProtocol": "openai",
    "providers": {
      "c4m": {
        "id": "c4m",
        "enabled": true,
        "type": "openai",
        "baseURL": "https://api.c4m.example.com/v1",
        "auth": {
          "type": "apikey",
          "apiKey": "${C4M_API_KEY}"
        },
        "apiKey": ["${C4M_API_KEY}"],
        "models": {
          "gpt-5.1": {
            "supportsStreaming": true
          }
        },
        "responses": {
          "process": "chat",
          "streaming": "always"
        },
        "compat": "responses:c4m"
      }
    },
    "routing": {
      "default": ["c4m.gpt-5.1"],
      "coding": [],
      "longcontext": [],
      "thinking": [],
      "tools": [],
      "vision": [],
      "websearch": [],
      "background": []
    }
  }
}
