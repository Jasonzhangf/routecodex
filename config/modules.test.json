{
  "modules": {
    "httpserver": {
      "enabled": true,
      "config": {
        "moduleType": "http-server",
        "port": 5516,
        "host": "127.0.0.1",
        "cors": {
          "origin": "*",
          "credentials": true
        },
        "timeout": 30000,
        "bodyLimit": "10mb",
        "enableMetrics": true,
        "enableHealthChecks": true,
        "logging": {
          "level": "info",
          "enableConsole": true,
          "enableFile": false,
          "categories": ["server", "api", "request", "config", "error", "message"]
        }
      }
    },
    "virtualrouter": {
      "enabled": true,
      "config": {
        "moduleType": "virtual-router",
        "inputProtocol": "openai",
        "outputProtocol": "openai",
        "routing": {
          "default": [
            "qwen.qwen3-coder-plus.token"
          ]
        },
        "providers": {
          "qwen": {
            "type": "qwen",
            "baseURL": "https://portal.qwen.ai/v1",
            "oauth": {
              "tokenFile": "/tmp/qwen-token.json"
            },
            "models": {
              "qwen3-coder-plus": {
                "maxTokens": 32768
              }
            }
          }
        }
      }
    }
  }
}
