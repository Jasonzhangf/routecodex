    "providers": {
      "glm": {
        "type": "openai",
        "baseURL": "https://open.bigmodel.cn/api/coding/paas/v4",
        "apiKey": [
          "6484fedc2cc9429e892dde8abf4c0bb8.es5PmJPf8XPvttZB"
        ],
        "models": {
          "glm-4.6": {
            "maxContext": 200000,
            "maxTokens": 8192,
