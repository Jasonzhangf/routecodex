{
  "dryRunEngine": {
    "enabled": true,
    "requestPipeline": {
      "dryRunMode": "partial",
      "nodeConfigs": {
        "llm-switch": {
          "enabled": true,
          "mode": "full-analysis",
          "breakpointBehavior": "continue",
          "verbosity": "detailed",
          "captureExecution": true,
          "inputSimulation": {
            "enabled": true,
            "strategy": "smart-defaults",
            "fallback": "empty-object"
          }
        },
        "compatibility": {
          "enabled": true,
          "mode": "full-analysis",
          "breakpointBehavior": "pause",
          "verbosity": "detailed",
          "captureExecution": true,
          "inputSimulation": {
            "enabled": true,
            "strategy": "copy-input",
            "fallback": "empty-object"
          }
        },
        "provider": {
          "enabled": false,
          "mode": "output-validation",
          "breakpointBehavior": "continue",
          "verbosity": "normal"
        }
      }
    },
    "responsePipeline": {
      "dryRunMode": "full",
      "responseDryRun": {
        "inputSource": "real-response",
        "cachingStrategy": "memory",
        "transformAnalysis": true
      },
      "nodeConfigs": {
        "response-compatibility": {
          "enabled": true,
          "mode": "full-analysis",
          "breakpointBehavior": "continue",
          "verbosity": "detailed",
          "analyzeTransformations": true,
          "captureExecution": true
        },
        "response-llm-switch": {
          "enabled": true,
          "mode": "full-analysis",
          "breakpointBehavior": "continue",
          "verbosity": "detailed",
          "analyzeTransformations": true,
          "captureExecution": true
        }
      }
    },
    "driverFeedback": {
      "analysisLevel": "detailed",
      "generateTimeline": true,
      "includeMemoryUsage": true,
      "performanceThresholds": {
        "warning": 5000,
        "critical": 10000
      }
    }
  },
  "lmstudio": {
    "baseUrl": "http://localhost:1234",
    "apiKey": "",
    "timeout": 60000,
    "retryAttempts": 3,
    "models": {
      "gpt-oss-20b-mlx": {
        "maxTokens": 262144,
        "temperature": 0.7,
        "supportsStreaming": true,
        "supportsTools": true
      },
      "qwen3-4b-thinking-2507-mlx": {
        "maxTokens": 262144,
        "temperature": 0.7,
        "supportsStreaming": true,
        "supportsTools": true
      }
    }
  }
}