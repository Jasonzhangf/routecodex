{
  "version": "1.0.0",
  "environment": "development",
  "debug": false,
  "logLevel": "info",
  "server": {
    "host": "0.0.0.0",
    "port": 3000,
    "cors": {
      "enabled": true,
      "origin": "*",
      "credentials": true,
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
      "allowedHeaders": ["Content-Type", "Authorization", "X-Requested-With"]
    },
    "rateLimit": {
      "enabled": true,
      "windowMs": 60000,
      "max": 100,
      "skipSuccessfulRequests": false,
      "skipFailedRequests": false
    },
    "compression": {
      "enabled": true,
      "threshold": 1024
    },
    "timeout": {
      "request": 30000,
      "response": 30000,
      "keepAlive": 5000
    }
  },
  "providers": {
    "openai-provider": {
      "id": "openai-provider",
      "type": "openai",
      "enabled": false,
      "baseUrl": "https://api.openai.com/v1",
      "timeout": 30000,
      "maxRetries": 3,
      "retryDelay": 1000,
      "models": {
        "gpt-4": {
          "maxTokens": 8192,
          "temperature": 0.7,
          "supportsStreaming": true,
          "supportsFunctions": true,
          "supportsVision": false
        },
        "gpt-4-turbo": {
          "maxTokens": 128000,
          "temperature": 0.7,
          "supportsStreaming": true,
          "supportsFunctions": true,
          "supportsVision": true
        },
        "gpt-3.5-turbo": {
          "maxTokens": 4096,
          "temperature": 0.7,
          "supportsStreaming": true,
          "supportsFunctions": true,
          "supportsVision": false
        }
      },
      "healthCheck": {
        "enabled": true,
        "interval": 30000,
        "timeout": 5000
      }
    },
    "anthropic-provider": {
      "id": "anthropic-provider",
      "type": "anthropic",
      "enabled": false,
      "baseUrl": "https://api.anthropic.com",
      "timeout": 30000,
      "maxRetries": 3,
      "retryDelay": 1000,
      "models": {
        "claude-3-sonnet-20240229": {
          "maxTokens": 200000,
          "temperature": 0.7,
          "supportsStreaming": true,
          "supportsFunctions": false,
          "supportsVision": true
        },
        "claude-3-opus-20240229": {
          "maxTokens": 200000,
          "temperature": 0.7,
          "supportsStreaming": true,
          "supportsFunctions": false,
          "supportsVision": true
        }
      },
      "healthCheck": {
        "enabled": true,
        "interval": 30000,
        "timeout": 5000
      }
    },
    "pass-through-provider": {
      "id": "pass-through-provider",
      "type": "pass-through",
      "enabled": true,
      "timeout": 30000,
      "maxRetries": 3,
      "retryDelay": 1000,
      "models": {
        "default": {
          "maxTokens": 4096,
          "temperature": 0.7,
          "supportsStreaming": true,
          "supportsFunctions": false,
          "supportsVision": false
        }
      },
      "healthCheck": {
        "enabled": true,
        "interval": 30000,
        "timeout": 5000
      }
    }
  },
  "routing": {
    "strategy": "round-robin",
    "defaultProvider": "pass-through-provider",
    "fallbackProvider": "pass-through-provider",
    "rules": [
      {
        "pattern": "^/v1/openai/chat/completions",
        "provider": "pass-through-provider",
        "priority": 1
      },
      {
        "pattern": "^/v1/openai/completions",
        "provider": "pass-through-provider",
        "priority": 1
      }
    ],
    "loadBalancing": {
      "enabled": true,
      "algorithm": "round-robin",
      "weights": {},
      "healthCheck": {
        "enabled": true,
        "interval": 30000,
        "timeout": 5000,
        "unhealthyThreshold": 3,
        "healthyThreshold": 2
      }
    }
  },
  "dynamicRouting": {
    "enabled": false,
    "categories": {
      "default": {
        "targets": []
      },
      "longcontext": {
        "enabled": false,
        "targets": []
      },
      "thinking": {
        "enabled": false,
        "targets": []
      },
      "background": {
        "enabled": false,
        "targets": []
      },
      "websearch": {
        "enabled": false,
        "targets": []
      },
      "vision": {
        "enabled": false,
        "targets": []
      },
      "coding": {
        "enabled": false,
        "targets": []
      }
    }
  },
  "security": {
    "authentication": {
      "enabled": false,
      "type": "api-key"
    },
    "authorization": {
      "enabled": false,
      "type": "rbac",
      "rules": []
    },
    "encryption": {
      "enabled": false,
      "algorithm": "aes-256-gcm",
      "keyRotationDays": 90
    },
    "rateLimit": {
      "enabled": true,
      "requests": 100,
      "windowMs": 60000,
      "skipSuccessfulRequests": false,
      "skipFailedRequests": false
    },
    "cors": {
      "enabled": true,
      "origin": "*",
      "credentials": true,
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
      "allowedHeaders": ["Content-Type", "Authorization", "X-Requested-With"],
      "exposedHeaders": [],
      "maxAge": 86400
    }
  },
  "monitoring": {
    "enabled": true,
    "metrics": {
      "enabled": true,
      "endpoint": "/metrics",
      "interval": 10000
    },
    "logging": {
      "level": "info",
      "format": "json",
      "outputs": [
        {
          "type": "console",
          "config": {}
        }
      ]
    },
    "tracing": {
      "enabled": false,
      "sampler": 1.0,
      "exporter": "console"
    },
    "health": {
      "enabled": true,
      "endpoint": "/health",
      "detailed": true
    }
  },
  "cache": {
    "enabled": false,
    "type": "memory",
    "ttl": 300000,
    "maxSize": 1000,
    "compression": false
  },
  "modules": {
    "httpserver": {
      "enabled": true,
      "config": {}
    },
    "configmanager": {
      "enabled": true,
      "config": {}
    },
    "providermanager": {
      "enabled": true,
      "config": {}
    },
    "requesthandler": {
      "enabled": true,
      "config": {}
    },
    "responsehandler": {
      "enabled": true,
      "config": {}
    },
    "openairouter": {
      "enabled": true,
      "config": {}
    },
    "anthropicrouter": {
      "enabled": false,
      "config": {}
    },
    "errorhandling": {
      "enabled": true,
      "config": {}
    },
    "debugcenter": {
      "enabled": true,
      "config": {}
    }
  }
}