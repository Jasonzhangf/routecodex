{
  "version": "1.0.0",
  "virtualrouter": {
    "inputProtocol": "openai",
    "outputProtocol": "openai",
    "providers": {
      "mock": {
        "id": "mock",
        "enabled": true,
        "type": "mock-provider",
        "providerType": "responses",
        "baseURL": "https://mock.local/mock",
        "compatibilityProfile": "compat:passthrough",
        "auth": {
          "type": "apikey",
          "keys": {
            "key1": {
              "value": "mock-sample-key"
            }
          }
        },
        "models": {
          "mock-model": { "maxTokens": 8192 }
        }
      }
    },
    "routing": { "default": ["mock.key1.mock-model"] }
  },
  "httpserver": { "host": "127.0.0.1", "port": 5560 }
}
