{
  "virtualrouter": {
    "inputProtocol": "openai",
    "outputProtocol": "openai",
    "routing": {
      "default": [
        "qwen-provider.qwen-turbo.qwen-oauth-config",
        "iflow-provider.iflow-gpt4.iflow-oauth-config"
      ],
      "longcontext": [
        "qwen-provider.qwen-max.qwen-long-context",
        "iflow-provider.iflow-gpt4.iflow-long-context"
      ]
    },
    "providers": {
      "qwen-provider": {
        "type": "qwen-provider",
        "baseURL": "https://dashscope.aliyuncs.com/compatible-mode/v1",
        "apiKey": [
          "auth-qwen-oauth-config",
          "auth-qwen-long-context"
        ],
        "auth": {
          "qwen-oauth-config": "~/.routecodex/auth/qwen.json"
        },
        "oauth": {
          "qwen-oauth-config": {
            "clientId": "your-qwen-client-id",
            "clientSecret": "your-qwen-client-secret",
            "authUrl": "https://dashscope.aliyuncs.com/api/v1/oauth/authorize",
            "tokenUrl": "https://dashscope.aliyuncs.com/api/v1/oauth/token",
            "deviceCodeUrl": "https://dashscope.aliyuncs.com/api/v1/oauth/device_code",
            "scopes": ["openid", "profile", "api"],
            "enablePKCE": true,
            "apiBaseUrl": "https://dashscope.aliyuncs.com"
          },
          "qwen-long-context": {
            "clientId": "your-qwen-client-id",
            "clientSecret": "your-qwen-client-secret-for-long-context",
            "authUrl": "https://dashscope.aliyuncs.com/api/v1/oauth/authorize",
            "tokenUrl": "https://dashscope.aliyuncs.com/api/v1/oauth/token",
            "deviceCodeUrl": "https://dashscope.aliyuncs.com/api/v1/oauth/device_code",
            "scopes": ["openid", "profile", "api", "long-context"],
            "enablePKCE": true,
            "apiBaseUrl": "https://dashscope.aliyuncs.com"
          }
        },
        "models": {
          "qwen-turbo": {
            "maxContext": 128000,
            "maxTokens": 32000
          },
          "qwen-max": {
            "maxContext": 2000000,
            "maxTokens": 65536
          }
        }
      },
      "iflow-provider": {
        "type": "iflow-http",
        "baseURL": "https://api.iflow.cn/v1",
        "apiKey": [
          "auth-iflow-oauth-config",
          "auth-iflow-long-context"
        ],
        "auth": {
          "iflow-oauth-config": "~/.routecodex/auth/iflow.json"
        },
        "oauth": {
          "iflow-oauth-config": {
            "clientId": "10009311001",
            "clientSecret": "4Z3YjXycVsQvyGF1etiNlIBB4RsqSDtW",
            "authUrl": "https://iflow.cn/oauth",
            "tokenUrl": "https://iflow.cn/oauth/token",
            "deviceCodeUrl": "https://iflow.cn/oauth/device/code",
            "scopes": ["openid", "profile", "api"],
            "enablePKCE": true,
            "useLegacyCredentials": true,
            "apiBaseUrl": "https://api.iflow.cn/v1"
          },
          "iflow-long-context": {
            "clientId": "10009311001",
            "clientSecret": "4Z3YjXycVsQvyGF1etiNlIBB4RsqSDtW",
            "authUrl": "https://iflow.cn/oauth",
            "tokenUrl": "https://iflow.cn/oauth/token",
            "deviceCodeUrl": "https://iflow.cn/oauth/device/code",
            "scopes": ["openid", "profile", "api", "long-context"],
            "enablePKCE": true,
            "useLegacyCredentials": true,
            "apiBaseUrl": "https://api.iflow.cn/v1"
          }
        },
        "models": {
          "iflow-gpt4": {
            "maxContext": 128000,
            "maxTokens": 32000
          }
        }
      }
    }
  },
  "user": {
    "name": "OAuth User",
    "email": "oauth@example.com"
  }
}
