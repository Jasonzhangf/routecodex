{
  "version": "1.0.0",
  "httpserver": {
    "host": "127.0.0.1",
    "port": 5545
  },
  "virtualrouter": {
    "providers": {
      "mimo": {
        "id": "mimo",
        "enabled": true,
        "type": "openai-standard",
        "baseURL": "https://api.xiaomimimo.com/v1",
        "auth": {
          "type": "apikey",
          "apiKey": "${MIMO_API_KEY}"
        },
        "models": {
          "mimo-v2-flash": {
            "supportsStreaming": true
          }
        }
      }
    },
    "routing": {
      "default": [
        {
          "id": "default-primary",
          "mode": "priority",
          "targets": ["mimo.mimo-v2-flash"]
        }
      ]
    }
  }
}
