{
  "version": "1.0.0",
  "httpserver": {
    "port": 5520,
    "host": "127.0.0.1"
  },
  "virtualrouter": {
    "providers": {
      "tabglm": {
        "id": "tabglm",
        "enabled": true,
        "type": "anthropic",
        "baseURL": "https://api.tabcode.cc/claude/glm",
        "process": "chat",
        "auth": {
          "type": "apikey",
          "apiKey": "${TABGLM_API_KEY}"
        },
        "models": {
          "glm-4.7": {
            "supportsStreaming": true
          }
        },
        "compatibilityProfile": "chat:claude-code"
      }
    },
    "routing": {
      "default": [
        {
          "id": "default-primary",
          "mode": "priority",
          "targets": ["tabglm.glm-4.7"]
        }
      ],
      "thinking": [
        {
          "id": "thinking-primary",
          "mode": "priority",
          "targets": ["tabglm.glm-4.7"]
        }
      ]
    }
  }
}
