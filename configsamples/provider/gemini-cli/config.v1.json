{
  "version": "1.0.0",
  "httpserver": {
    "port": 5522,
    "host": "127.0.0.1"
  },
  "virtualrouter": {
    "providers": {
      "gemini-cli": {
        "id": "gemini-cli",
        "enabled": true,
        "type": "gemini-cli-http-provider",
        "providerType": "gemini",
        "baseURL": "https://cloudcode-pa.googleapis.com",
        "auth": {
          "type": "gemini-oauth",
          "tokenFile": "~/.routecodex/auth/gemini-oauth.json"
        },
        "models": {
          "gemini-2.5-pro": {
            "maxTokens": 64000,
            "maxContext": 256000,
            "supportsStreaming": true
          }
        }
      }
    },
    "routing": {
      "default": [
        "gemini-cli.gemini-2.5-pro"
      ]
    }
  }
}
