{
  "version": "1.0.0",
  "httpserver": {
    "host": "127.0.0.1",
    "port": 5555
  },
  "virtualrouter": {
    "providers": {
      "openai": {
        "id": "openai",
        "enabled": true,
        "type": "openai",
        "baseURL": "https://api.openai.com/v1",
        "auth": {
          "type": "apikey",
          "apiKey": "${OPENAI_API_KEY}"
        },
        "models": {
          "gpt-5.2": {
            "supportsStreaming": true
          },
          "gpt-5.2-codex": {
            "supportsStreaming": true
          }
        }
      }
    },
    "routing": {
      "default": [
        {
          "id": "default-primary",
          "mode": "priority",
          "targets": [
            "openai.gpt-5.2"
          ]
        }
      ],
      "thinking": [
        {
          "id": "thinking-primary",
          "mode": "priority",
          "targets": [
            "openai.gpt-5.2"
          ]
        }
      ],
      "tools": [
        {
          "id": "tools-primary",
          "mode": "priority",
          "targets": [
            "openai.gpt-5.2"
          ]
        }
      ]
    }
  }
}
