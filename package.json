{
  "name": "routecodex",
  "version": "0.0.1",
  "description": "Multi-provider OpenAI proxy server",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "npm run clean && tsc",
    "build:watch": "tsc --watch",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "clean": "rm -rf dist",
    "prebuild": "echo 'Skipping lint for now'",
    "prepare": "npm run build"
  },
  "dependencies": {
    "rcc-basemodule": "^0.2.2",
    "rcc-debugcenter": "^0.1.0",
    "rcc-errorhandling": "^1.0.5",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/jest": "^29.5.5",
    "typescript": "^5.0.0",
    "tsx": "^4.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "eslint": "^8.50.0",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4"
  }
}