{
  "name": "routecodex",
  "version": "0.2.7",
  "description": "Multi-provider OpenAI proxy server with native dry-run capabilities",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "bin": {
    "routecodex": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./adv-base-module": {
      "import": "./sharedmodule/rcc-basemodule-adv/adv-base-module.js",
      "types": "./sharedmodule/rcc-basemodule-adv/types.js"
    }
  },
  "files": [
    "dist/",
    "sharedmodule/rcc-basemodule-adv/",
    "config/",
    "scripts/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run clean && tsc",
    "build:watch": "tsc --watch",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "test": "NODE_OPTIONS=\"--experimental-vm-modules\" jest",
    "test:watch": "NODE_OPTIONS=\"--experimental-vm-modules\" jest --watch",
    "test:adv-module": "NODE_OPTIONS=\"--experimental-vm-modules\" jest tests/adv-base-module-*.test.js",
    "test:dry-run": "npm run test:adv-module && node tests/basic-dry-run.mjs",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "clean": "rm -rf dist",
    "prebuild": "echo 'Skipping lint for now'",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build && npm test",
    "install:global": "./scripts/simple-install.sh",
    "install:quick": "./scripts/simple-install.sh",
    "install:skip-tests": "./scripts/simple-install.sh",
    "dry-run": "node scripts/dry-run-cli.mjs",
    "test:lmstudio-dryrun": "node tests/lmstudio-tools-bidir-dry-run.mjs"
  },
  "dependencies": {
    "@lmstudio/sdk": "^1.5.0",
    "@types/socket.io": "^3.0.1",
    "axios": "^1.12.2",
    "chalk": "^5.6.2",
    "commander": "^14.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "lmstudio": "^0.0.32",
    "node-fetch": "^3.3.2",
    "openai": "^5.23.0",
    "ora": "^9.0.0",
    "rcc-basemodule": "^0.2.7",
    "rcc-basemodule-adv": "^0.0.1",
    "rcc-debugcenter": "^0.1.5",
    "rcc-errorhandling": "^1.0.10",
    "socket.io": "^4.8.1"
  },
  "peerDependencies": {
    "@jest/globals": "^30.1.2"
  },
  "peerDependenciesMeta": {
    "@jest/globals": {
      "optional": true
    }
  },
  "devDependencies": {
    "@jest/globals": "^30.1.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@typescript-eslint/typescript-estree": "^6.7.4",
    "depcheck": "^1.4.3",
    "eslint": "^8.50.0",
    "husky": "^9.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "git add"
    ]
  }
}
