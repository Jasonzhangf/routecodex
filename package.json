{
  "name": "routecodex",
  "version": "0.2.7",
  "description": "Multi-provider OpenAI proxy server with native dry-run capabilities",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "bin": {
    "routecodex": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./adv-base-module": {
      "import": "./sharedmodule/rcc-basemodule-adv/adv-base-module.js",
      "types": "./sharedmodule/rcc-basemodule-adv/types.js"
    }
  },
  "files": [
    "dist/",
    "sharedmodule/rcc-basemodule-adv/",
    "config/",
    "scripts/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run clean && tsc",
    "build:watch": "tsc --watch",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:performance": "jest --testPathPattern=performance",
    "test:adv-module": "NODE_OPTIONS=\"--experimental-vm-modules\" jest tests/adv-base-module-*.test.js",
    "test:dry-run": "npm run test:adv-module && node tests/basic-dry-run.mjs",
    "test:lmstudio-dryrun": "node tests/lmstudio-tools-bidir-dry-run.mjs",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "lint:strict": "eslint src --ext .ts --max-warnings 0",
    "clean": "rm -rf dist coverage",
    "prebuild": "npm run lint:fix && npm run test",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build && npm test",
    "install:global": "./scripts/simple-install.sh",
    "install:quick": "./scripts/simple-install.sh",
    "install:skip-tests": "./scripts/simple-install.sh",
    "dry-run": "node scripts/dry-run-cli.mjs",
    "security:audit": "npm audit --audit-level high",
    "security:check": "npx audit-ci --config ./audit-ci.json",
    "complexity:check": "npx plato -r -d complexity-report src/",
    "duplication:check": "npx jscpd --threshold 5 src/",
    "secrets:check": "echo 'Secrets check temporarily disabled'",
    "format:check": "prettier --check src/**/*.{ts,js,json}",
    "format:fix": "prettier --write src/**/*.{ts,js,json}",
    "depcheck": "depcheck",
    "license:check": "license-checker --onlyAllow 'MIT;Apache-2.0;BSD-2-Clause;BSD-3-Clause;ISC;0BSD;Python-2.0;CC-BY-4.0;CC-BY-3.0;CC0-1.0;Unlicense'",
    "ncu:check": "ncu --errorLevel 2",
    "package:validate": "npx package-json-validator"
  },
  "dependencies": {
    "@lmstudio/sdk": "^1.5.0",
    "@types/socket.io": "^3.0.1",
    "axios": "^1.12.2",
    "chalk": "^5.6.2",
    "commander": "^14.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "lmstudio": "^0.0.32",
    "node-fetch": "^3.3.2",
    "openai": "^5.23.0",
    "ora": "^9.0.0",
    "rcc-basemodule": "^0.2.7",
    "rcc-basemodule-adv": "^0.0.1",
    "rcc-debugcenter": "^0.1.6",
    "rcc-errorhandling": "^1.0.10",
    "socket.io": "^4.8.1"
  },
  "peerDependencies": {
    "@jest/globals": "^30.1.2"
  },
  "peerDependenciesMeta": {
    "@jest/globals": {
      "optional": true
    }
  },
  "devDependencies": {
    "@jest/globals": "^30.1.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@typescript-eslint/typescript-estree": "^6.7.4",
    "coveralls": "^3.1.1",
    "depcheck": "^1.4.7",
    "eslint": "^8.50.0",
    "eslint-plugin-complexity": "^1.0.2",
    "husky": "^9.0.0",
    "jest": "^29.7.0",
    "jscpd": "^4.0.5",
    "license-checker": "^25.0.1",
    "lint-staged": "^15.0.0",
    "npm-check-updates": "^18.3.0",
    "package-json-validator": "^0.30.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "git add"
    ]
  }
}
