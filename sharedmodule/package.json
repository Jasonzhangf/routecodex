{
  "name": "@routecodex/sharedmodule",
  "version": "0.1.1",
  "description": "RouteCodex Shared Configuration Modules",
  "main": "index.js",
  "type": "module",
  "workspaces": [
    "config-engine",
    "config-compat",
    "config-testkit"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "test": "npm run test --workspaces",
    "lint": "npm run lint --workspaces",
    "clean": "npm run clean --workspaces",
    "dev": "npm run dev --workspaces",
    "publish:all": "npm run build && npm publish --workspaces",
    "setup": "npm install && npm run build"
  },
  "keywords": [
    "routecodex",
    "configuration",
    "ai",
    "proxy",
    "llm"
  ],
  "author": "RouteCodex Team",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0",
    "vitest": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/routecodex/routecodex.git",
    "directory": "sharedmodule"
  },
  "bugs": {
    "url": "https://github.com/routecodex/routecodex/issues"
  },
  "homepage": "https://github.com/routecodex/routecodex#readme"
}
